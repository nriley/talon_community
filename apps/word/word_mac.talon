app: word_mac
-
tag(): user.find_and_replace

paste special: key(cmd-ctrl-v)

align left: key(cmd-l)
align center: key(cmd-e)

bold: key(cmd-b)
italic: key(cmd-i)
underline: key(cmd-u)
strike through: key(cmd-shift-x)

mail this: user.menu_select("File|Share|Send Document")
mail pdf: user.menu_select("File|Share|Send PDF")

ribbon: key(cmd-alt-r)

comments switch: user.toggle_comments()
